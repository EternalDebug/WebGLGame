@page
@model Scene
@{
    ViewData["Title"] = "Underwater world";
}
<script src="../js/Scene1/vsShaders.js"></script>
<script src="../js/Scene1/fsShaders.js"></script>
<script src="../js/Scene1/Main_Script.js"></script>


<div style="position: fixed; top: 70px; left: 20px; text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.5); font-family: 'comic sans ms';">
    <p id="coord">x = 0; z = -4;</p>
    <p><input type="checkbox" id="lightingSpace"> Фонарь 1</p>
    <div>
        <p>SunX</p>
        <input id="sunX" type="range" min="-100.0" max="100.0" step="0.5">
    </div>
    <div>
        <p>SunY</p>
        <input id="sunY" type="range" min="-100" max="100" step="0.5">
    </div>
    <div>
        <p>SunZ</p>
        <input id="sunZ" type="range" min="-100" max="100" step="0.5">
    </div>
    <p><input type="checkbox" id="lightingSpace2"> Фонарь 2</p>
    <div>
        <p>SunX</p>
        <input id="sunX2" type="range" min="-100.0" max="100.0" step="0.5">
    </div>
    <div>
        <p>SunY</p>
        <input id="sunY2" type="range" min="-100" max="100" step="0.5">
    </div>
    <div>
        <p>SunZ</p>
        <input id="sunZ2" type="range" min="-100" max="100" step="0.5">
    </div>
</div>
<canvas id="glcanvas" width="1000" height="1000"></canvas>

@Model.OBJStoHTML("Field", "octopus", "teasure", "scorpion", "fish")

